{
  "name": "PokeDOM",
  "version": "1.0.0",
  "description": "Boilerplate for Webpack applications",
  "private": true,
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf dist",
    "start": "NODE_ENV=development node -r dotenv/config devServer.js",
    "api": "NODE_ENV=development nodemon -r dotenv/config apiServer.js",
    "test": "jest --coverage --forceExit",
    "watch-test": "jest --verbose --watch --notify",
    "lint": "eslint '**/**.js'",
    "migration:migrate:test": "NODE_ENV=test node -r dotenv/config node_modules/knex/bin/cli.js migrate:latest",
    "migration:gen": "node -r dotenv/config node_modules/knex/bin/cli.js migrate:make",
    "migration:migrate": "node -r dotenv/config node_modules/knex/bin/cli.js migrate:latest",
    "migration:rollback": "node -r dotenv/config node_modules/knex/bin/cli.js migrate:rollback"
  },
  "author": "Cristian Cepeda",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-sass-loader": "^1.0.10",
    "chart.js": "^1.1.1",
    "cors": "^2.8.1",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "influx": "^4.2.2",
    "jquery": "^3.1.1",
    "knex": "^0.12.2",
    "kue": "^0.11.1",
    "node-sass": "^3.10.1",
    "node-schedule": "^1.1.1",
    "pg": "^6.1.0",
    "raven-js": "^3.7.0",
    "rc-chartjs": "^0.7.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.75.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "socket.io": "^1.4.8",
    "socket.io-client": "https://registry.npmjs.org/socket.io-client/-/socket.io-client-1.4.8.tgz",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^11.2.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "jest-cli": "file:vendor/node_modules/jest-cli-16.0.0.tgz",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "sinon": "^1.17.6",
    "supertest": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/testSetupFile.js",
    "testRegex": "/__tests__/.*\\.spec\\.js$"
  }
}
