{
  "name": "kommit-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Webpack applications",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "clean": "rimraf dist",
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --recursive --reporter spec test",
    "watch-test": "NODE_ENV=test mocha --recursive --watch --reporter spec test",
    "lint": "eslint '**/**.js'"
  },
  "author": "Cristian Cepeda",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-sass-loader": "^1.0.10",
    "chart.js": "~1.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "jquery": "^3.1.0",
    "node-sass": "^3.9.3",
    "raven-js": "^3.7.0",
    "rc-chartjs": "^0.7.1",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.5",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "redis": "^2.6.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
